<?xml version="1.0" encoding="UTF-8" ?>
<Shell
  x:Class="Animeikan.GUI.AppShell"
  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  xmlns:views="clr-namespace:Animeikan.GUI.Views"
  xmlns:models="clr-namespace:Animeikan.GUI.Models" 
  xmlns:constants="clr-namespace:Animeikan.GUI.Globals" 
  BackgroundColor="{x:StaticResource BaseColor20B}"
  FlyoutBehavior="Disabled">

  <ShellContent
    Title="OnboardingPage"
    ContentTemplate="{DataTemplate views:OnboardingPage}"
    Route="OnboardingPage">
  </ShellContent>
  
  <ShellContent
    Title="MainPage"
    ContentTemplate="{DataTemplate views:MainPage}"
    Route="MainPage">
  </ShellContent>

</Shell>
